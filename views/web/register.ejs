<body>
	
	
	<div class="container-login100" style="background-image: url('/web/images/bg-01.jpg');">
		<div class="wrap-login100 p-l-55 p-r-55 p-t-80 p-b-30 full-wrap">
            <a href="/" class="home-btn">Home</a>

			<form class="login100-form validate-form" action="RegisterUser" method="POST">
				<span class="login100-form-title p-b-37">
					Register
                </span>
                
                <div class="wrap-input100 validate-input m-b-20" data-validate="Enter email">
					<input class="input100" type="text" name="email" placeholder="email">
					<span class="focus-input100"></span>
				</div>

				<div class="wrap-input100 validate-input m-b-20" data-validate="Enter username">
					<input class="input100" type="text" name="uname" placeholder="username">
					<span class="focus-input100"></span>
				</div>

				<div class="wrap-input100 validate-input m-b-25" data-validate = "Enter password">
					<input class="input100" type="password" name="upass" placeholder="password">
					<span class="focus-input100"></span>
				</div>

				<div class="container-login100-form-btn">
					<button class="login100-form-btn">
						Register
					</button>
				</div>

		
				<div class="flex-c p-b-112">
				
				
				</div>

				<div class="text-center">
					<a href="/User" class="txt2 hov1">
						Sign In
					</a>
				</div>
			</form>

			
		</div>
